{
  "name": "ngx-decorators-examples",
  "description": "Example app demonstrating Decorators for Angular (ngx-decorators).",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Lars Gyrup Brink Nielsen",
    "email": "larsbrinknielsen@gmail.com",
    "url": "https://github.com/LayZeeDK"
  },
  "homepage": "https://github.com/LayZeeDK/ngx-decorators",
  "bugs": "https://github.com/LayZeeDK/ngx-decorators/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/LayZeeDK/ngx-decorators.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-decorators --prod",
    "test": "ng test",
    "lint": "run-p lint:e2e lint:examples lint:lib",
    "lint:e2e": "ng lint ngx-decorators-examples-ng6-e2e --type-check --force",
    "lint:examples": "ng lint ngx-decorators-examples-ng6 --type-check --force",
    "lint:lib": "ng lint ngx-decorators --type-check --force",
    "e2e": "ng e2e",
    "watch": "npm-watch"
  },
  "watch": {
    "lint": {
      "patterns": [
        "projects/ngx-decorators/src/lib",
        "projects/ngx-decorators-examples-ng6/src",
        "projects/ngx-decorators-examples-ng6-e2e/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:e2e": {
      "patterns": [
        "projects/ngx-decorators-examples-ng6-e2e/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:examples": {
      "patterns": [
        "projects/ngx-decorators-examples-ng6/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:lib": {
      "patterns": [
        "projects/ngx-decorators/src/lib"
      ],
      "extensions": [
        "ts"
      ]
    }
  },
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.6",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.10.13",
    "codelyzer": "^4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.3.0",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tsickle": ">=0.28.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
